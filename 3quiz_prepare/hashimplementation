#include <bits/stdc++.h>
using namespace std;
int main(){
    int a[] = {5 , 28 , 19 , 15 , 20 , 33 , 12 , 17 , 10}
    int n = a.size() ; 
    list<int>hashTable[7] ;
    for(int i = 0 ; i < n ; i ++){
        int k = a[i] % 7 ; 
        hashTable[k].push_back(a[i]); 
    }
    for(int i = 0 ; i < 7 ; i ++){
        cout << "Slot " << i <<  " "; 
        for(int x : HashTable[i]){
            cout << x << " " ; 
        }
        cout << endl ; 
    }
}